<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="comviewzonazul.google.httpssites.zonazul.usuario.gui.LoginActivity">


    <EditText
        android:id="@+id/login_edtUsuario"
        android:layout_width="249dp"
        android:layout_height="39dp"
        android:layout_margin="100dp"
        android:layout_weight="0.08"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Login"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="184dp" />

    <EditText
        android:id="@+id/login_edtSenha"
        android:layout_width="249dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Senha"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="236dp" />

    <Button
        android:id="@+id/login_btnEntrar"
        android:layout_width="98dp"
        android:layout_height="39dp"
        android:onClick="@string/login_btnEntrar"
        android:text="Login"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="286dp" />

    <Button
        android:id="@+id/login_btnCadastrar"
        android:layout_width="92dp"
        android:layout_height="24dp"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="336dp" />

</android.support.constraint.ConstraintLayout>
